<h1>Audit Logs Dashboard</h1>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <table class="table table-hover table-striped">
        <thead>
          <tr>
            <th>
              #
            </th>
            <th>
              Employee
            </th>
            <th>
              Week starting
            </th>
            <th>
              Confirmed At
            </th>
            <th>
              Status
            </th>
<!--             <th>
            </th>
            <th>
            </th> -->
          </tr>
        </thead>
        <tbody id="audit_logs">
          <%= render @audit_logs %>
        </tbody>
      </table>
    </div>
  </div>
</div>

<div id="paginator">
  <%= paginate @audit_logs, remote: true %>
</div>

